{
  "name": "botified-client",
  "displayName": "botified-client",
  "description": "AI-powered Visual Studio Code generator",
  "version": "0.0.2",
  "type": "module",
  "publisher": "botified-extension",
  "repository": {
    "type": "git",
    "url": "https://github.com/gabsss1/botified-client-vsc"
  },
  "engines": {
    "vscode": "^1.101.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "code-assistant.openChat",
        "title": "Abrir Chat Botificado"
      }
    ],
    "keybindings": [
      {
        "command": "code-assistant.openChat",
        "key": "ctrl+cmd+0",
        "mac": "ctrl+cmd+0",
        "win": "ctrl+alt+0"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.101.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.25.1"
  },
  "dependencies": {
    "axios": "^1.10.0"
  }
}
